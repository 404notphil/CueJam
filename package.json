{
  "name": "cuejam",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "ENVFILE=.env.development react-native start",
    "test": "jest",
    "kn": "killall node",
    "adb-r": "adb reverse tcp:8081 tcp:8081",
    "tron": "adb reverse tcp:9090 tcp:9090",
    "resetc": "yarn start -- --reset-cache",
    "node:clean": "rm -rf node_modules&& yarn cache clean && yarn install",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "ios:Pod:Reset": "cd ios && pod deintegrate && pod setup && pod install",
    "ios:clean": "yarn node:clean &&  cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && yarn ios:Pod:Reset",
    "ios:Pod:install": "cd ios && pod install && cd ..",
    "ios:bundle:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:dev": "yarn setDevelopment  react-native run-ios --mode=Debug --scheme \"development\"",
    "ios:dev-release": "yarn ios:clean && yarn setDevelopment  react-native run-ios --mode=Release --scheme \"development\"",
    "ios:staging": "yarn setStaging  react-native run-ios --mode=Debug --scheme \"staging\"",
    "ios:staging-release": "yarn ios:clean && yarn setStaging  react-native run-ios --mode=Release --scheme \"staging\"",
    "ios:prod": "yarn setProduction  react-native run-ios --mode=Debug --scheme \"production\"",
    "ios:prod-release": " yarn ios:clean && yarn ios:bundle:assets && yarn setProduction  react-native run-ios --mode=Release --scheme \"production\"",
    "android:clean": "yarn node:clean && cd android && ./gradlew clean",
    "android:bundle:assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:remove:assets": "rm -rf android/app/src/main/res/drawable-hdpi &&  rm -rf android/app/src/main/res/drawable-mdpi &&  rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw",
    "android:dev": "yarn setDevelopment  react-native run-android --mode=debug --appIdSuffix=development",
    "android:dev-release": "yarn setDevelopment  react-native run-android --mode=developmentRelease--appIdSuffix=development",
    "android:staging": "yarn setStaging  react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "android:staging-release": "yarn setStaging  react-native run-android --mode=stagingRelease --appIdSuffix=staging",
    "android:prod": "yarn setProduction  react-native run-android --mode=release",
    "android:prod-release": "yarn setProduction  react-native run-android --mode=productionRelease --appIdSuffix=production",
    "android:d-apk": "yarn setDevelopment &&  yarn android:clean && yarn android:bundle:assets && yarn android:remove:assets cd android && ./gradlew assembleDebug && killall -9 java",
    "android:p-apk": "yarn setProduction &&  yarn android:clean && yarn android:bundle:assets && cd android && ./gradlew assembleRelease && killall -9 java",
    "open-apk": "open ./android/app/build/outputs/apk/",
    "install-apk": "adb install ./android/app/build/outputs/apk/release/app-release.apk",
    "open-bundle": "open ./android/app/build/outputs/bundle/productionRelease",
    "android:build-install-prod-apk": "yarn android:p-apk && adb install ./android/app/build/outputs/apk/release/app-release.apk",
    "android:dev-apk": "yarn android:d-apk && yarn open-apk",
    "android:dev:release-apk": "yarn android:p-apk && yarn open-apk",
    "android:staging-apk": "yarn android:SD-apk && yarn open-apk",
    "android:staging:release-apk": "yarn android:SR-apk && yarn open-apk",
    "android:prod-apk": "yarn android:p-apk && yarn open-apk",
    "android:prod:release-apk": "yarn android:p-apk && yarn open-apk",
    "android:bundle": "yarn android:p-bundle && yarn open-bundle",
    "deleteXcodeDerivedData": "rm -rf ${HOME}/Library/Developer/Xcode/DerivedData/*",
    "clearDotEnv": "rm -f node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-firebase/app": "^20.3.0",
    "@react-native-firebase/auth": "^20.3.0",
    "@react-native-firebase/firestore": "^20.3.0",
    "@react-native-picker/picker": "^2.7.7",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "@reduxjs/toolkit": "^2.2.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.12.3",
    "react-native-picker-select": "^9.1.3",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.1.0",
    "react-native-uuid": "^2.0.2",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.6",
    "@types/react-native-sqlite-storage": "^6.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
